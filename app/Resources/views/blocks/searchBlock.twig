{% block searchBlock %}
<input id="searchbox" class="form-control form-control-sm mt-1 ml-2" type="text" placeholder="Buscar">

{% for label in ["usuario", "descripcion", "lugar"] %}
	

	<div class="form-check form-check-inline">
  <input class="form-check-input" type="radio" name="options" id={{label}} value={{label}}>
  <label class="form-check-label" for="inlineRadio1">{{label}}</label>
</div>
	
{% endfor %}

{% endblock %}




