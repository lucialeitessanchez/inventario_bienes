{% block searchBlock %}
<input id="searchbox" class="form-control form-control-sm mt-1 ml-2" type="text" placeholder="Buscar">
<div class="ml-2 form-check-inline">
{% for label in ["usuario", "descripcion", "lugar"] %}
	<div class="form-check form-check-inline">
		<input class="form-check-input" type="checkbox" id={{label}} checked="checked" />
		<label class="form-check-label" for="inlineCheckbox1">{{label}}</label>
	</div>
{% endfor %}
</div>
{% endblock %}




